#import "YSSGreenBorderButton.h"

@implementation YSSGreenBorderButton

- (instancetype)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        [self.layer setBorderWidth:0.5];
        [self.layer setBorderColor:MALLColor.CGColor];
        self.layer.cornerRadius = 2.5;
        [self setTitleColor:MALLColor forState:UIControlStateNormal];
        self.backgroundColor = [UIColor clearColor];
        [self setTitleColor:LIGHTTEXTCOLOR forState:UIControlStateDisabled];
    }
    return self;
}



@end
