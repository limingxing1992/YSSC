- (void)viewDidAppear:(BOOL)animated{
    [super viewDidAppear:animated];
    
    [self.useShellSwi setOn:YES];//默认打开余额支付，不管余额多少
    _isUseFiveShell = YES;
    BOOL ret = [self okUseBlanceMoneyPayForOrder];
    if (ret) {
